[
  {
    "namespace": "monitoring",
    "name": "alertmanager-main",
    "type": "ClusterIP",
    "selector": "alertmanager=main|app=alertmanager",
    "port": "9093/TCP"
  },
  {
    "namespace": "monitoring",
    "name": "node-exporter",
    "type": "ClusterIP",
    "selector": "app.kubernetes.io/name=node-exporter",
    "port": "9100/TCP"
  },
  {
    "namespace": "monitoring",
    "name": "grafana",
    "type": "ClusterIP",
    "selector": "app=grafana",
    "port": "3000/TCP"
  },
  {
    "namespace": "monitoring",
    "name": "prometheus-server-lb",
    "type": "LoadBalancer",
    "selector": "app=prometheus|prometheus=k8s",
    "port": "9090:31756/TCP"
  },
  {
    "namespace": "monitoring",
    "name": "kube-state-metrics",
    "type": "ClusterIP",
    "selector": "app.kubernetes.io/name=kube-state-metrics",
    "port": "8443/TCP,9443/TCP"
  }
]
